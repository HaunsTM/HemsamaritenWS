  <PropertyGroup>
    <PostBuildEvent>SET solutionDir=$(SolutionDir)
SET targetDir=$(TargetDir)
SET targetFileName=$(TargetFileName)

SET hemsamaritenIP=10.0.0.2
SET remoteShare=\\hemsamariten\hemsamaritenWS
SET remoteWindowsServiceName=HemsamaritenWindowsService

REM Special copy logic for Audio project
if "$(ConfigurationName)" == "Deploy to Hemsamariten" goto copyAudioFiles
if "$(ConfigurationName)" == "Run locally on Hemsamariten" goto copyAudioFiles
goto copyAudioFilesDone
:copyAudioFiles
robocopy "$(SolutionDir)Tellstick\Audio\Media" "$(TargetDir)Media" /is /mir /e
if %25errorlevel%25 leq 1 exit 0 else exit %25errorlevel%25
:copyAudioFilesDone

if "$(ConfigurationName)" == "Deploy to Hemsamariten"  (  c:\Windows\System32\WindowsPowerShell\v1.0\powershell.exe -NoProfile -ExecutionPolicy Bypass -Command "&amp; '$(ProjectDir)deployToHemsamariten.ps1' -solutionDir  '%25solutionDir%25 ' -targetDir '%25targetDir%25 ' -targetFileName '%25targetFileName%25 ' -hemsamaritenIP '%25hemsamaritenIP%25' -remoteShare '%25remoteShare%25' -remoteWindowsServiceName '%25remoteWindowsServiceName%25'";)
if "$(ConfigurationName)" == "Run locally on Hemsamariten"  (  c:\Windows\System32\WindowsPowerShell\v1.0\powershell.exe -NoProfile -ExecutionPolicy Bypass -Command "&amp; '$(ProjectDir)runLocallyOnHemsamariten.ps1' -solutionDir  '%25solutionDir%25 ' -targetDir '%25targetDir%25 ' -targetFileName '%25targetFileName%25 ' -hemsamaritenIP '%25hemsamaritenIP%25' -remoteShare '%25remoteShare%25' -remoteWindowsServiceName '%25remoteWindowsServiceName%25'";)</PostBuildEvent>
